<!DOCTYPE html>
<head>
    <title>All POsts</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1>Quara Posts</h1>
    <%for(let post of posts){%>
        <div class="post">
            <h3 class="user">@<%=post.username%> </h3>
            <h4 class="contenet"><%=post.content%></h4>
        </div>
    <%}%>
    <a href="http://localhost:3000/posts/new">Create new Posts</a>
</body>
</html>